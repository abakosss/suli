{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: 1001 Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/<relative-path-to-project-folder>1001.csproj"
        },
        {
            "name": "C#: 1004 Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/<relative-path-to-project-folder>1004.csproj"
        },
        {
            // Use IntelliSense to find out which attributes exist for C# debugging
            // Use hover for the description of the existing attributes
            // For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/asztesmobalk/1001/bin/Debug/net8.0/1001.dll",
            "args": [],
            "cwd": "${workspaceFolder}/asztesmobalk/1001",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "C#: 0917",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/frontend/0917/0917.csproj",
            "launchConfigurationId": "TargetFramework=;0917"
        }
    ]
}